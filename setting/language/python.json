{
    "language": "cpp",
    "trait":"ALL_SPACE|ALL_ENTER|SEM_ENTER",
	"pre_def0":"[\n:(){}]",
	"pre_def1":"[a-zA-Z0-9_]",
    "pre_def2":"[^\"]",
    "pre_def3":"[a-zA-Z]_",
    "pre_def4":"[0-9]",
    "pre_def5":"[^\\\"\n]",
    "pre_def6":"[^\\']",
    "pre_def7":"[\\\"]",
    "pre_def8":"[a-fA-F0-9]",
    "pre_def9":"[uUlLfF]",
    "pre_def10":"[^\\'\n]",
    "pre_def11":"[\t ]",
    "sub_pattern1":"[\\][%*]",
    "sub_pattern2":"[\"%e][\"%e][\"]",
    "sub_pattern3":"[\\]['%e]",
    "sub_pattern4":"['%e]['%e]'",
    "note":"#[^\n%m]|\"[\"][\"][^\"%m%n][$2%n][^\"%k%x]|'[']['][^'%m%n][$4%n][^'%k%x]",
    "string":"\"\"|\"[@5%m][$1%m%n%b][\"%t]|'[@10%m][$1%b%m%n]['%t]|''",
    "number":"0[x%c][@8%m]|[@4][@4%m][@9%m]|[@4%m][.%m][@4][@4%m][@9%m]",
    "keyword":"print|and|as|assert|break|continue|def|del|exec|finally|from|global|import|in|is|lambda|not|or|pass|print|raise|return|with|yield|self|__name__|None",
    "keyword1":"def",
    "keyword2":"if|elif|else|for|while|class|try|except",
    "sem_predef1":"[^:\t\n]",
    "pre_wrapper":"[#k2][@1%m][:%t]",
    "function_def": "[#k1][#v%r%t]",
    "function_call":"[#v%r][(%t]", 
    "variables":"[@3][@1%m]"
}